{
	"name": "icodec",
	"version": "0.2.0",
	"description": "WebAssembly image encoders & decoders",
	"license": "MIT",
	"author": "Kaciras <Kaciras@outlook.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/Kaciras/icodec"
	},
	"type": "module",
	"keywords": [
		"WebAssembly",
		"image",
		"optimize",
		"converter",
		"webp",
		"avif",
		"qoi",
		"jxl",
		"wp2",
		"mozjpeg",
		"pngquant"
	],
	"files": [
		"dist/*.{js,wasm}",
		"lib/**/*.{js,d.ts}"
	],
	"exports": {
		".": "./lib/index.js",
		"./node": {
			"types": "./lib/index.d.ts",
			"default": "./lib/node.js"
		},
		"./png-enc.wasm": "./dist/png_bg.wasm",
		"./qoi-enc.wasm": "./dist/qoi.wasm",
		"./qoi-dec.wasm": "./dist/qoi.wasm",
		"./*.wasm": "./dist/*.wasm"
	},
	"devDependencies": {
		"@kaciras/eslint-config-core": "^3.1.0",
		"@kaciras/eslint-config-typescript": "^3.1.0",
		"@types/emscripten": "^1.39.13",
		"@types/node": "^20.14.11",
		"esbench": "^0.6.1",
		"eslint": "^9.7.0",
		"pixelmatch": "^6.0.0",
		"sharp": "^0.33.4",
		"typescript": "^5.5.3"
	}
}
